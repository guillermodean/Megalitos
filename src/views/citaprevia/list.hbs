            <div class="card text-center">
                <div class="card-body">
                    <a href="{{telefono}}" target="_blank">
                        <h3 class="card-title text-uppercase">
                            {{nombre_tienda}}
                        </h3>
                    </a>
                    <p class="m-2">{{direccion}}</p>
                    <h5>Telefono para cita previa:</h5>
                    <a href="tel:{{telefono}}">{{telefono}}</a>
